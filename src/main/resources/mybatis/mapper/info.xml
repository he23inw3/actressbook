<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.hiro72.mapper.InfoMapper">
    <select id="selectActressList" resultType="jp.hiro72.mapper.model.InfoRecord">
        SELECT
            INFO_ID,
            TITLE,
            CATEGORY,
            INFO_URL,
            CREATED_DATE
        FROM
            TBL_INFORMATION
        ORDER BY
            CREATED_DATE DESC;
    </select>
</mapper>